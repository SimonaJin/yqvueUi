#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/bin/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules:/Users/cici/workpace/study/zf/z-plus/node_modules:/Users/cici/workpace/study/zf/node_modules:/Users/cici/workpace/study/node_modules:/Users/cici/workpace/node_modules:/Users/cici/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/bin/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/esbuild@0.13.8/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules/.pnpm/node_modules:/Users/cici/workpace/study/zf/z-plus/play/node_modules:/Users/cici/workpace/study/zf/z-plus/node_modules:/Users/cici/workpace/study/zf/node_modules:/Users/cici/workpace/study/node_modules:/Users/cici/workpace/node_modules:/Users/cici/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
