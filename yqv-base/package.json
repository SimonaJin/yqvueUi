{
    "name": "yqv-base",
    "version": "1.0.8",
    "description": "A Component Library for Vue.js.",
    "main": "lib/yqvue-ui.common.js",
    "scripts": {
        "relyon": "mnpm || npm i",
        "dev:play": "pnpm -C play dev",
        "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
        "build:file": "node build/bin/iconInit.js & node build/bin/build-entry.js",
        "build:theme": "gulp build --gulpfile packages/theme_chalk/gulpfile.js",
        "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
        "build:umd": "node build/bin/build-locale.js",
        "dist": "npm run clean && npm run build:file && npm run lint && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:utils && npm run build:theme",
        "lint": "eslint src/**/* test/**/* packages/**/**/**/** build/**/* --quiet",
        "test": "cross-env CI_ENV=/dev/ BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "test:watch": "npm run build:theme && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js"
    },
    "keywords": [
        "yqv",
        "vue",
        "components"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@yqv-base/components": "^1.0.0",
        "async-validator": "^4.0.7",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "deepmerge": "^4.2.2",
        "karma-chai": "^0.1.0",
        "throttle-debounce": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/preset-env": "^7.16.5",
        "@vue/component-compiler-utils": "^3.3.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^7.0.0-0",
        "babel-loader": "^7.1.5",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-istanbul": "^6.1.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "babel-regenerator-runtime": "^6.5.0",
        "chai": "^4.3.4",
        "copy-webpack-plugin": "^6.3.2",
        "cross-env": "^7.0.3",
        "css-loader": "^4.3.0",
        "es6-promise": "^4.2.8",
        "eslint": "^8.2.0",
        "eslint-config-elemefe": "^0.3.0",
        "eslint-plugin-html": "^6.2.0",
        "eslint-plugin-json": "^3.1.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-cssmin": "^0.2.0",
        "gulp-dart-sass": "^1.0.2",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.0.0",
        "html-webpack-plugin": "^4.5.2",
        "json-loader": "^0.5.7",
        "json-templater": "^1.2.0",
        "karma": "^6.3.8",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-mocha": "^2.0.1",
        "karma-sinon-chai": "^2.0.2",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-spec-reporter": "^0.0.32",
        "karma-webpack": "^4.0.2",
        "mini-css-extract-plugin": "^1.6.2",
        "mocha": "^9.1.3",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "sass": "^1.43.4",
        "sass-loader": "^8.0.2",
        "sinon": "^12.0.1",
        "sinon-chai": "^3.7.0",
        "style-loader": "^1.3.0",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "uppercamelcase": "^3.0.0",
        "url-loader": "^4.1.1",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.8",
        "vue-router": "^3.5.3",
        "vue-template-compiler": "^2.6.14",
        "vue-template-es2015-compiler": "^1.9.1",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^3.11.3"
    }
}
